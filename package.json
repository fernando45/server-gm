{
    "name": "API-GESTIONA-NODE",
    "version": "1.0.0",
    "description": "API de Gestiona creada con Node + TypeScript + GraphQL +Express + JWC + BCRIPT ",
    "main": "build/server.js",
    "scripts": {
        "start": "node build/server.js",
        "build": "tsc -p . && ncp src/schema build/schema",
        "start:dev": "npm run build:dev",
        "build:dev": "nodemon \"src/server.ts\" --exec \"ts-node\" src/server.ts -e ts,graphql"
    },
    "keywords": [
        "graphql",
        "graphql-curso",
        "hola-mundo"
    ],
    "author": "José Fernando González Tejedor",
    "license": "MIT",
    "dependencies": {
        "@graphql-tools/load-files": "^6.0.8",
        "@graphql-tools/merge": "^6.0.8",
        "@types/bcryptjs": "^2.4.2",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/mongodb": "^3.5.20",
        "@types/mongoose": "^5.7.22",
        "apollo-server-express": "^2.14.2",
        "bcryptjs": "^2.4.3",
        "chalk": "^3.0.0",
        "cloudinary": "^1.21.0",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "express": "^4.17.1",
        "express-graphql": "^0.9.0",
        "graphql": "^14.6.0",
        "graphql-import-node": "0.0.4",
        "graphql-merge-resolvers": "^1.1.16",
        "graphql-middleware": "^4.0.2",
        "graphql-playground-middleware-express": "^1.7.15",
        "graphql-shield": "^7.3.0",
        "graphql-tools": "^4.0.8",
        "http": "0.0.0",
        "jsonwebtoken": "^8.5.1",
        "merge-graphql-schemas": "^1.7.8",
        "mongodb": "^3.5.8",
        "mongoose": "^5.9.18",
        "ncp": "^2.0.0"
    },
    "devDependencies": {
        "@types/compression": "0.0.36",
        "@types/cors": "^2.8.6",
        "@types/express": "^4.17.6",
        "@types/express-graphql": "^0.8.2",
        "@types/graphql": "^14.5.0",
        "@types/node": "^12.12.44",
        "nodemon": "^2.0.4",
        "ts-node": "^8.10.2",
        "typescript": "^3.9.5"
